    "babel-core": "^5.8.25",
    "del": "^2.0.2",
    "expect.js": "^0.3.1",
    "gulp": "^3.9.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.4.1",
    "isparta": "^3.1.0",
    "istanbul": "^0.3.22",
    "mocha": "^2.3.3"
  },
  "dependencies": {
    "gulp-load-plugins": "^1.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wesleyanemam/gulp-es-next.git"
  },
  "bugs": {
